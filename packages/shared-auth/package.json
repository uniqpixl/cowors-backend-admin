{
  "name": "@cowors/shared-auth",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared authentication utilities and configurations for Cowors applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.esm.js",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.esm.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./config": {
      "import": "./dist/config/index.esm.js",
      "require": "./dist/config/index.js",
      "types": "./dist/config/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./nextauth": {
      "import": "./dist/nextauth/index.esm.js",
      "require": "./dist/nextauth/index.js",
      "types": "./dist/nextauth/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cowors",
    "authentication",
    "auth",
    "nextauth",
    "shared",
    "types",
    "utilities"
  ],
  "author": "Cowors Team",
  "license": "MIT",
  "dependencies": {
    "@cowors/shared-types": "workspace:*",
    "next-auth": "^4.24.11",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "next-auth": ">=4.24.0",
    "typescript": ">=4.9.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "jsdom": "^27.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.8.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.8.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cowors/cowors-mvp.git",
    "directory": "packages/shared-auth"
  }
}